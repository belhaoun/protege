    <!-- http://www.semanticweb.org/niklas/ontologies/publications#Affiliation -->

    <owl:Class rdf:about="http://www.semanticweb.org/niklas/ontologies/publications#Affiliation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#hasMember"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#Author"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#hasAffiliationName"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Affiliations are institutes or universities authors are affiliated to.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/niklas/ontologies/publications#Author -->

    <owl:Class rdf:about="http://www.semanticweb.org/niklas/ontologies/publications#Author">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#hasAffiliation"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#Affiliation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#isAuthorOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#Publication"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#hasAuthorName"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niklas/ontologies/publications#hasORCID"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
